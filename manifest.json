{
    "name": "Okta Workflows Helper",
    "description": "",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png",
      "default_title": "test"
    },
    "background":{
      "service_worker":"service-worker.js"
    },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        
        "https://ooo.workflows.oktapreview.com/*",
        "https://mickolas.workflows.okta.com/*",
        "https://*.workflows.okta.com/*",
        "https://*.workflows.oktapreview.com.com/*",
        "https://*.workflows.trexcloud.com/*"
      ]
    }
  ],
  "host_permissions": [
    
    "https://ooo.workflows.oktapreview.com/*",
    "https://mickolas.workflows.okta.com/*",
    "https://*.workflows.okta.com/*",
    "https://*.workflows.oktapreview.com.com/*",
    "https://*.workflows.trexcloud.com/*"

  ],
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "declarativeContent",
    "activeTab",
    "webRequest"
  ]

  }