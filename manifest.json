{
    "name": "Okta Workflows Helper",
    "description": "",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png",
      "default_title": "test"
    },
    "background":{
      "service_worker":"service-worker.js"
    },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "activeTab"
  ]

  }